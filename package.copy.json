{
  "name": "larkyo",
  "version": "1.0.0",
  "description": "larkyo client application",
  "main": "index.js",
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules dist",
    "clean:dist": "rimraf dist",
    "start": "npm run server:dev",
    "build": "cross-env NODE_ENV=development npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "server:dev": "cross-env NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --progress --profile --watch --host 0.0.0.0",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "webpack": "cross-env ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env ./node_modules/.bin/webpack-dev-server",
    "prebuild:prod": "npm run clean:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vio-lets/planner-app.git"
  },
  "keywords": [
    "larkyo",
    "react",
    "travel"
  ],
  "author": "larkyo developing team",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/vio-lets/planner-app/issues"
  },
  "homepage": "https://github.com/vio-lets/planner-app#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "css-modules-require-hook": "^4.0.6",
    "css-mqpacker": "^5.0.1",
    "csswring": "^5.1.1",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.16",
    "postcss-color-function": "^3.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-modules-values": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.1.0",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "normalize.css": "^6.0.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-router": "^4.0.0"
  }
}
